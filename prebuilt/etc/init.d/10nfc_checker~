#!/system/bin/sh

model=`getprop ro.product.model`

if [ "$model" = "LGLS740" ] || [ "$model" = "LG-D405" ]; then
	mount -o remount,rw /system
	chmod 644 /system/etc/firmware/keymaster.mdt
	chmod 644 /system/etc/firmware/keymaster.mdt
chmod 644 /system/etc/firmware/keymaster.mdt
chmod 644 /system/etc/firmware/keymaster.mdt
chmod 644 /system/etc/firmware/keymaster.mdt
	mount -o remount,ro /system
fi
